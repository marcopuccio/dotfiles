{
	"auto_indent": true,
	"color_scheme": "Packages/User/SublimeLinter/brogrammer (SL).tmTheme",
	"detect_indentation": true,
	"folder_exclude_patterns":
	[
		".svn",
		".git",
		".hg",
		".idea",
		".sass-cache",
		".happypack",
		"node_modules",
		"bower_components",
		"htmlcov",
		"env"
	],
	"font_size": 8,
	"git_binary":
	{
		"linux": "/usr/bin/git"
	},
	"hot_exit": true,
	"rulers":
	[
		79,
		119
	],
	"smart_indent": true,
	"tab_size": 4,
	"theme": "Brogrammer.sublime-theme",
	"translate_tabs_to_spaces": true
}
